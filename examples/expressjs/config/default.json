{
  "daLogger": {
    "level": "debug",
    "traceKeyName": "dalogger-trace-key",
    "provider": "pino",
    "settings": {
      "pino": {
        "transport": {
          "targets": [
            {
              "target": "pino/file",
              "options": {
                "destination": "/tmp/dalogger-demo-express.log",
                "append": true
              }
            }
          ]
        }
      },
      "winston": {
        "transports": [
          {
            "module": "file",
            "args": {
              "filename": "/tmp/dalogger-demo-express.log"
            }
          }
        ]
      }
    }
  }
}
